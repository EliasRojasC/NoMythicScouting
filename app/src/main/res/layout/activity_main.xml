<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.elias.nomythicscouting.MainActivity">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="565dp"
        android:layout_height="507dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="15dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="1.0">

        <Button
            android:id="@+id/button10AddGear"
            android:layout_width="57dp"
            android:layout_height="52dp"
            android:layout_marginTop="32dp"
            android:text="+"
            android:textSize="24dp"
            app:layout_constraintTop_toBottomOf="@+id/button5SubtractLow"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <Button
            android:id="@+id/button9SubtractGear"
            android:layout_width="57dp"
            android:layout_height="52dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="32dp"
            android:text="-"
            android:textSize="24dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button6AddLow" />

        <Button
            android:id="@+id/button8AddHigh"
            android:layout_width="57dp"
            android:layout_height="52dp"
            android:layout_marginLeft="16dp"
            android:text="-"
            android:textSize="24dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="111dp" />

        <Button
            android:id="@+id/button7SubtractHigh"
            android:layout_width="57dp"
            android:layout_height="52dp"
            android:layout_marginTop="111dp"
            android:text="+"
            android:textSize="24dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent" />

        <Button
            android:id="@+id/button6AddLow"
            android:layout_width="57dp"
            android:layout_height="52dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="32dp"
            android:text="-"
            android:textSize="24dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button8AddHigh" />

        <Button
            android:id="@+id/button5SubtractLow"
            android:layout_width="57dp"
            android:layout_height="52dp"
            android:layout_marginTop="32dp"
            android:text="+"
            android:textSize="24dp"
            app:layout_constraintTop_toBottomOf="@+id/button7SubtractHigh"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <TextView
            android:id="@+id/gearLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:layout_marginTop="16dp"
            android:text="  Gears  "
            android:textSize="18dp"
            app:layout_constraintBottom_toTopOf="@+id/gearCount"
            app:layout_constraintTop_toBottomOf="@+id/lowCount"
            app:layout_constraintVertical_bias="0.833"
            tools:layout_editor_absoluteX="127dp" />

        <TextView
            android:id="@+id/gearCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textSize="30dp"
            tools:layout_editor_absoluteY="279dp"
            app:layout_constraintRight_toLeftOf="@+id/button10AddGear"
            android:layout_marginRight="16dp"
            app:layout_constraintLeft_toRightOf="@+id/button9SubtractGear"
            android:layout_marginLeft="16dp"
            app:layout_constraintHorizontal_bias="0.519" />

        <TextView
            android:id="@+id/highShot"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="High Shots"
            android:textSize="18dp"
            tools:layout_editor_absoluteY="74dp"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.225" />

        <TextView
            android:id="@+id/highCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textSize="30dp"
            app:layout_constraintLeft_toRightOf="@+id/button8AddHigh"
            android:layout_marginLeft="16dp"
            app:layout_constraintRight_toLeftOf="@+id/button7SubtractHigh"
            android:layout_marginRight="16dp"
            tools:layout_editor_absoluteY="111dp"
            app:layout_constraintHorizontal_bias="0.519" />

        <TextView
            android:id="@+id/lowShot"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:layout_marginTop="16dp"
            android:text="Low Shots"
            android:textSize="18dp"
            app:layout_constraintBottom_toTopOf="@+id/lowCount"
            app:layout_constraintTop_toBottomOf="@+id/highCount"
            app:layout_constraintVertical_bias="0.0"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.232" />

        <TextView
            android:id="@+id/lowCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textSize="30dp"
            app:layout_constraintRight_toLeftOf="@+id/button5SubtractLow"
            android:layout_marginRight="16dp"
            app:layout_constraintLeft_toRightOf="@+id/button6AddLow"
            android:layout_marginLeft="16dp"
            app:layout_constraintHorizontal_bias="0.519"
            tools:layout_editor_absoluteY="195dp" />

        <TextView
            android:id="@+id/textView"
            style="@style/Widget.AppCompat.TextView.SpinnerItem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:text="TeleOp"
            android:textSize="30sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </android.support.constraint.ConstraintLayout>

    <android.support.constraint.ConstraintLayout
        android:layout_width="562dp"
        android:layout_height="388dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:id="@+id/constraintLayout2"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Low Shot"
            android:textSize="18dp"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.501"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/editText2HighGoal"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/editTextLowGoal" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:text="High Shot"
            android:textSize="18dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.501"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/editText2HighGoal" />

        <Button
            android:id="@+id/button4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="16dp"
            android:layout_marginTop="40dp"
            android:text="+"
            android:textSize="24dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button" />

        <Button
            android:id="@+id/button3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="39dp"
            android:text="-"
            android:textSize="24dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button2" />

        <TextView
            android:id="@+id/editText2HighGoal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Name"
            android:textSize="24dp"
            app:layout_constraintLeft_toRightOf="@+id/button2"
            android:layout_marginLeft="16dp"
            app:layout_constraintRight_toLeftOf="@+id/button"
            android:layout_marginRight="16dp"
            app:layout_constraintTop_toTopOf="@+id/textView3"
            android:layout_marginTop="16dp" />

        <TextView
            android:id="@+id/editTextLowGoal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Name"
            android:textSize="24dp"
            app:layout_constraintLeft_toRightOf="@+id/button3"
            android:layout_marginLeft="16dp"
            app:layout_constraintRight_toLeftOf="@+id/button4"
            android:layout_marginRight="16dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="251dp" />

        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="155dp"
            android:text="-"
            android:textSize="24dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="16dp"
            android:text="+"
            android:textSize="24dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="155dp" />

        <CheckBox
            android:id="@+id/checkBox"
            style="@android:style/Widget.Holo.CompoundButton.CheckBox"
            android:layout_width="172dp"
            android:layout_height="57dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:text="Gear In Auto?"
            app:layout_constraintLeft_toLeftOf="parent"
            android:textSize="24dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2" />

        <TextView
            android:id="@+id/textView2"
            style="@style/Widget.AppCompat.TextView.SpinnerItem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="16dp"
            android:text="Auto"
            android:textSize="30sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>
